node
{


        stage('Code Checkout')
        {

        git 'https://github.com/mohitvohra1190/Devops3'
        }


    stage('Compile')
    {
         echo 'before compiling'

        def mvnHome = tool name: 'maven_3_6_3', type: 'maven'

        echo "${mvnHome}"
                bat = "${mvnHome}/bin/mvn clean compile"
                echo 'after compiling'

     }

      

     stage ('testing')
     {
         def mvnHome = tool name: 'maven_3_6_3', type: 'maven'
         echo 'before testing'
        mvn test
        
         echo 'after testing'
         always{
        junit 'Test/*.xml'

         }
     }



}
